        2320,
        1344
      ],
      "credentials": {
        "googleDriveOAuth2Api": {
          "id": "40qs0Hm9t9b8bx7u",
          "name": "Google Drive account"
        }
      }
    },
    {
      "parameters": {
        "conditions": {
          "options": {
            "caseSensitive": true,
            "leftValue": "",
            "typeValidation": "strict",
            "version": 3
          },
          "conditions": [
            {
              "id": "condition-1",
              "leftValue": "={{ $json.createdTime }}",
              "rightValue": "={{ $('Workflow Configuration').item.json.todayStart }}",
              "operator": {
                "type": "dateTime",
                "operation": "afterOrEquals"
              }
            },
            {
              "id": "condition-2",
              "leftValue": "={{ $json.createdTime }}",
              "rightValue": "={{ $('Workflow Configuration').item.json.todayEnd }}",
              "operator": {
                "type": "dateTime",
                "operation": "beforeOrEquals"
              }
            }
          ],
          "combinator": "and"
        },
        "options": {}
      },
      "id": "eac0f3b6-88d7-4701-98f1-50cf688d50fb",
      "name": "Filter Today's Videos Only",
