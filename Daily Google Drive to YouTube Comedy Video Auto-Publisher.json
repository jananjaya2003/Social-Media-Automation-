      "typeVersion": 3.4,
      "position": [
        2096,
        1344
      ]
    },
    {
      "parameters": {
        "resource": "fileFolder",
        "searchMethod": "query",
        "queryString": "={{ 'mimeType contains \"video/\" and \"' + $json.folderId + '\" in parents and trashed=false' }}",
        "returnAll": true,
        "filter": {},
        "options": {
          "fields": [
            "*"
          ]
        }
      },
      "id": "1ebf5796-7bb2-4db7-9693-a80b54e9f6ae",
